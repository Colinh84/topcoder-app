n\n  &:disabled {\n    opacity: .3;\n    cursor: default;\n  }\n\n  &.tc-btn-secondary {\n    border-color: $gray;\n    background-color: $gray;\n    background-image : none;\n\n    &:hover {\n      border-color: $primary;\n      background-color: $primary;\n      background-image : none;\n    }\n\n    &:active {\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      background-image : none;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n  }\n\n  &.tc-btn-ghost {\n    color: $primary;\n    background-color: $white;\n\n    &:hover {\n      color: $white;\n      border-color: $primary;\n      background-color: $primary;\n    }\n\n    &:active {\n      color: $white;\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n\n    &:disabled {\n      border-color: $gray-dark;\n      color: $gray-dark;\n    }\n  }\n\n  &.tc-btn-ghost-secondary {\n    color: $primary;\n    background-color: $white;\n    border-color: $gray;\n\n    &:hover {\n      border-color: $primary;\n    }\n\n    &:active {\n      color: $white;\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n\n    &:disabled {\n      border-color: $gray-dark;\n      color: $gray-dark;\n    }\n  }\n\n  &.tc-btn-warning {\n    &:hover {\n      background-color: $error;\n      border-color: $error-dark;\n\n    }\n    &:active {\n      background-color: $error-dark;\n\n    }\n  }\n\n\n\n  &.tc-btn-xs {\n    height: 20px;\n    padding: 0 10px;\n    line-height: 20px;\n    font-size: 11px;\n    border-radius: 3px;\n\n    &:active {\n      line-height: 21px;\n    }\n  }\n\n  &.tc-btn-s {\n    height: 30px;\n    padding: 0 10px;\n    line-height: 28px;\n    font-size: 12px;\n    font-weight: 500;\n\n    &:active {\n      line-height: 29px;\n    }\n  }\n\n  &.tc-btn-l {\n    height: 50px;\n    padding