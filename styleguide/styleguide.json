ext-dark: #464646;\n$member-search-highlight: #F69C23;\n$member-tile-border: #E0E0E0;\n$member-blue: #03B7FF;\n$member-button: #00AEFF;\n$member-design: $member-blue;\n$member-develop: #A3D200;\n$member-data_science: #F49F2A;\n\n$statistics-design: #22AEFF;\n$statistics-develop: #99cc00;\n$statistics-data_science: #F39426;\n$statistics-table-even: #FAFAFA;","syntax":"scss","file":"_tc-colors.scss","parentReference":"1","wrapper":"<sg-wrapper-content/>","renderMarkup":"<div style=\"background: {$modifiers};\" class=\"styleguide-color\">{$modifiers}</div>","variables":["design","data_science","develop","copilot"],"fileHash":"db358f5eb03673e48339446faf95cf00"},{"header":"The default, primary topcoder button.","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"2","markup":null,"css":"a.tc-btn, button.tc-btn {\n  height: 40px;\n  padding: 0 15px;\n  line-height: 40px;\n  border-radius: 4px;\n  border: 1px solid $primary;\n  background-color: $primary;\n  background-image : none;\n  @include sofia-pro-regular;\n  font-size: 16px;\n  color: $white;\n  text-transform: uppercase;\n  outline: none;\n  text-shadow: none;\n\n  &:hover {\n    background-color: $primary-dark;\n    border-color: $primary-dark;\n    color: $white;\n  }\n\n  &:active,\n  &:focus {\n    background-color: $primary-dark;\n    background-image : none;\n    border-color: $primary-dark;\n    box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    line-height: 30px;\n  }\n\n  &:disabled {\n    opacity: .3;\n    cursor: default;\n  }\n\n  &.tc-btn-secondary {\n    border-color: $gray;\n    background-color: $gray;\n    background-image : none;\n\n    &:hover {\n      border-color: $primary;\n      background-color: $primary;\n      background-image : none;\n    }\n\n    &:active {\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      background-image : none;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n  }\n\n  &.tc-btn-ghost {\n    color: $primary;\n    background-color: $white;\n\n    &:hover {\n      color: $white;\n      border-color: $primary;\n      background-color: $primary;\n    }\n\n    &:active {\n      color: $white;\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n\n    &:disabled {\n      border-color: $gray-dark;\n      color: $gray-dark;\n    }\n  }\n\n  &.tc-btn-ghost-secondary {\n    color: $primary;\n    background-color: $white;\n    border-color: $gray;\n\n    &:hover {\n      border-color: $primary;\n    }\n\n    &:active {\n      color: $white;\n      border-color: $primary-dark;\n      background-color: $primary-dark;\n      box-shadow: inset 0px 1px 1px 0px rgba(0,0,0,0.30);\n    }\n\n    &:disabled {\n      border-color: $gray-dark;\n      color: $gray-dark;\n    }\n  }\n\n  &.tc-btn-warning {\n    &:hover {\n      background-color: $error;\n      border-color: $error-dark;\n\n    }\n    &:active {\n      background-color: $error-dark;\n\n    }\n  }\n\n\n\n  &.tc-btn-xs {\n    height: 20px;\n    padding: 0 10px;\n    line-height: 20px;\n    font-size: 11px;\n    border-radius: 3px;\n\n    &:active {\n      line-height: 21px;\n    }\n  }\n\n  &.tc-btn-s {\n    height: 30px;\n    padding: 0 10px;\n    line-height: 28px;\n    font-size: 12px;\n    font-weight: 500;\n\n    &:active {\n      line-height: 29px;\n    }\n  }\n\n  &.tc-btn-l {\n    height: 50px;\n    padding: 0 25px;\n    line-height: 50px;\n    font-size: 18px;\n\n    &:active {\n      line-height: 51px;\n    }\n  }\n\n  &.tc-btn-xl {\n    height: 60px;\n    padding: 0 30px;\n    line-height: 60px;\n    font-size: 24px;\n\n    &:active {\n      line-height: 61px;\n    }\n  }\n\n  &.tc-btn-wide {\n    padding: 0 30px;\n  }\n}","syntax":"scss","file":"_tc-buttons.scss","wrapper":"<sg-wrapper-content/>","variables":["primary","primary","white","primary-dark","primary-dark","white","primary-dark","primary-dark"